<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.taiton.entity.DetailsPaymentEntity">
        <table name="details_payment" schema="taitonbankdb" catalog=""/>
        <attributes>
            <id name="id">
                <column name="Id" precision="11"/>
            </id>
            <basic name="info">
                <column name="Info" length="45"/>
            </basic>
            <one-to-many name="serviceHasDetailsPaymentsById" mapped-by="detailsPaymentByDetailsPaymentId"
                         target-entity="com.taiton.entity.ServiceHasDetailsPaymentEntity"/>
        </attributes>
    </entity>
</entity-mappings>
