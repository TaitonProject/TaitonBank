<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.taiton.entity.SessionInformationEntity">
        <table name="session_information" schema="ibankdb" catalog=""/>
        <attributes>
            <id name="id">
                <column name="Id" precision="11"/>
            </id>
            <basic name="sessionStart">
                <column name="Session_Start" precision="-1"/>
            </basic>
            <basic name="sessionEnd">
                <column name="Session_End" precision="-1"/>
            </basic>
            <basic name="userId">
                <column name="User_Id" precision="11"/>
            </basic>
            <many-to-one name="userByUserId" optional="true" target-entity="com.taiton.entity.UserEntity">
                <join-column name="User_Id" referenced-column-name="Id"/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
