<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.taiton.entity.UserInfoEntity">
        <table name="user_info" schema="taitonbankdb" catalog=""/>
        <attributes>
            <id name="id">
                <column name="Id" precision="11"/>
            </id>
            <basic name="firstName">
                <column name="First_Name" length="45"/>
            </basic>
            <basic name="secondName">
                <column name="Second_Name" length="45"/>
            </basic>
            <basic name="surName">
                <column name="Sur_Name" length="45"/>
            </basic>
            <basic name="userId">
                <column name="User_Id" precision="11"/>
            </basic>
            <basic name="passportNumber">
                <column name="Passport_Number" length="9"/>
            </basic>
            <basic name="authority">
                <column name="Authority" length="45"/>
            </basic>
            <basic name="dateOfIssue">
                <column name="Date_Of_Issue" precision="-1"/>
            </basic>
            <many-to-one name="userByUserId" optional="true" target-entity="com.taiton.entity.UserEntity">
                <join-column name="User_Id" referenced-column-name="Id"/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
