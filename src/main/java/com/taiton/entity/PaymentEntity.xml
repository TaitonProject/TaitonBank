<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.taiton.entity.PaymentEntity">
        <table name="payment" schema="taitonbankdb" catalog=""/>
        <attributes>
            <id name="id">
                <column name="Id" precision="11"/>
            </id>
            <basic name="date">
                <column name="Date" precision="-1"/>
            </basic>
            <basic name="info">
                <column name="Info" nullable="false"/>
            </basic>
            <basic name="amount">
                <column name="Amount" precision="-1"/>
            </basic>
            <basic name="accountId">
                <column name="Account_Id" precision="11"/>
            </basic>
            <basic name="serviceId">
                <column name="Service_Id" precision="11"/>
            </basic>
            <basic name="save">
                <column name="Save" precision="1"/>
            </basic>
            <many-to-one name="accountByAccountId" optional="true" target-entity="com.taiton.entity.AccountEntity">
                <join-column name="Account_Id" referenced-column-name="Id"/>
            </many-to-one>
            <many-to-one name="serviceByServiceId" optional="true" target-entity="com.taiton.entity.ServiceEntity">
                <join-column name="Service_Id" referenced-column-name="Id"/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
