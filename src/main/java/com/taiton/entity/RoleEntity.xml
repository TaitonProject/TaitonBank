<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.taiton.entity.RoleEntity">
        <table name="role" schema="taitonbankdb" catalog=""/>
        <attributes>
            <id name="id">
                <column name="Id" precision="11"/>
            </id>
            <basic name="nameRole">
                <column name="Name_Role" length="45"/>
            </basic>
            <one-to-many name="usersById" mapped-by="roleByRoleIdRole" target-entity="com.taiton.entity.UserEntity"/>
        </attributes>
    </entity>
</entity-mappings>
